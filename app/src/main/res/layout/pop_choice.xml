<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/contentView"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_centerHorizontal="true"
    android:layout_margin="@dimen/five"
    android:background="@color/white"
    android:gravity="bottom|center_horizontal">

    <LinearLayout
        android:id="@+id/ll_close"
        android:layout_width="match_parent"
        android:layout_height="@dimen/fortyf"
        android:layout_alignParentBottom="true"
        android:background="@android:color/white"
        android:gravity="center"
        android:orientation="horizontal">

        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_gravity="center"
            android:padding="8dp"
            android:src="@mipmap/ic_publish_delete" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/kehu_ll"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@id/ll_close"
        android:layout_alignLeft="@id/xinz_ll"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="60dp"
        android:layout_marginBottom="80dp"
        android:gravity="center"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/kehu_iv"
            android:layout_width="@dimen/thirtyf"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:src="@mipmap/icon_sale" />

        <TextView
            android:id="@+id/kehu_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="客户记录"
            android:textColor="@color/black"
            android:textSize="@dimen/text_eleven" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/zulin_ll"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/ll_close"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="60dp"
        android:layout_marginBottom="80dp"
        android:gravity="center"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/zulin_iv"
            android:layout_width="@dimen/thirtyf"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:src="@mipmap/icon_sale" />

        <TextView
            android:id="@+id/zulin_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="租赁车辆"
            android:textColor="@color/black"
            android:textSize="@dimen/text_eleven" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/dingd_ll"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@id/ll_close"
        android:layout_alignRight="@id/wans_ll"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="60dp"
        android:layout_marginBottom="80dp"
        android:gravity="center"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/dingd_iv"
            android:layout_width="@dimen/thirty"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:src="@mipmap/icon_sale" />

        <TextView
            android:id="@+id/dingd_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="订单状态"
            android:textColor="@color/black"
            android:textSize="@dimen/text_eleven" />

    </LinearLayout>


    <LinearLayout
        android:id="@+id/xinz_ll"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@id/kehu_ll"
        android:layout_centerHorizontal="true"
        android:layout_marginRight="@dimen/forty"
        android:layout_toLeftOf="@id/zhengx_ll"
        android:gravity="center"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/xinz_iv"
            android:layout_width="@dimen/thirty"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:src="@mipmap/icon_sale" />

        <TextView
            android:id="@+id/xinz_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="新增征信"
            android:textColor="@color/black"
            android:textSize="@dimen/text_eleven" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/zhengx_ll"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/zulin_ll"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="37dp"
        android:layout_marginRight="37dp"
        android:gravity="center"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/zhengx_iv"
            android:layout_width="@dimen/thirtyf"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:src="@mipmap/icon_sale" />

        <TextView
            android:id="@+id/zhengx_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="征信进度"
            android:textColor="@color/black"
            android:textSize="@dimen/text_eleven" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/wans_ll"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@id/dingd_ll"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="@dimen/forty"
        android:layout_toRightOf="@id/zhengx_ll"
        android:gravity="center"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/wans_iv"
            android:layout_width="@dimen/thirtyf"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:src="@mipmap/icon_sale" />

        <TextView
            android:id="@+id/wans_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="完善信息"
            android:textColor="@color/black"
            android:textSize="@dimen/text_eleven" />

    </LinearLayout>
</RelativeLayout>